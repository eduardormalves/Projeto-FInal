algoritmo "Exercicio1"
// Função :
// Autor :
// Data : 15/04/2025
// Seção de Declarações 
var
     b, h, area, perimetro:real
inicio

      escreva("Base: ")
      leia(b)
      escreva("Altura: ")
      leia(h)
      
      area := b * h
      perimetro := 2 * (b + h)
      
      escreval("Area: ", area)
      escreval("Perimetro: ", perimetro)
fimalgoritmo