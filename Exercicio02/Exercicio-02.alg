algoritmo "Exercicio2"
// Função :
// Autor :
// Data : 15/04/2025
// Seção de Declarações 
var
     l, area, perimetro:real
inicio

      escreva("Lado: ")
      leia(l)
      
      area := l * l
      perimetro := 4 * l
      
      escreval("Area: ", area)
      escreval("Perimetro: ", perimetro)
fimalgoritmo